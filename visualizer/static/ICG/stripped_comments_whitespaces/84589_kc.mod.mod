NEURON{SUFFIXkcUSEIONkREADekWRITEikUSEIONcaREADcaiRANGEgbarGLOBALk_oi,vhalf_oi,tmin_oiGLOBALk_ic,vhalf_ic,tmin_icGLOBALk_oc,vhalf_oc,tmin_ocGLOBALk_co,vhalf_co,tmin_co,tmax_co,alpha_co}UNITS{(mA)=(milliamp)(mV)=(millivolt)(molar)=(1/liter)(mM)=(millimolar)}ASSIGNED{v(mV)ek(mV)cai(mM)ik(mA/cm2)roi(/ms)ric(/ms)roc(/ms)rco(/ms)}PARAMETER{gbar=1.0(mho/cm2)k_oi=12(mV)k_ic=-10(mV)k_co=6(mV)k_oc=-3(mV)vhalf_oi=-30(mV)vhalf_ic=-120(mV)vhalf_co=-45(mV)vhalf_oc=-40(mV)tmin_oi=1.5e0(/ms)tmin_ic=1e-1(/ms)tmin_co=2e-2(/ms)tmin_oc=1.5e0(/ms)tmax_co=50(/ms)alpha_co=2.5e10(/mM/mM/mM)}STATE{COI}BREAKPOINT{SOLVEkinMETHODsparseik=gbar*O*(v-ek)}INITIAL{SOLVEkinSTEADYSTATEsparse}KINETICkin{rates(v)~C<->O(rco,roc)~O<->I(roi,0.0)~I<->C(ric,0.0)CONSERVEC+O+I=1}PROCEDURErates(v(mV)){roi=rate_tmax_inf(v,vhalf_oi,k_oi,tmin_oi)ric=rate_tmax_inf(v,vhalf_ic,k_ic,tmin_ic)roc=rate_tmax_inf(v,vhalf_oc,k_oc,tmin_oc)rco=rate_tmax_fin(v,vhalf_co,k_co,tmin_co,tmax_co)*alpha_co*cai^3}FUNCTIONrate_tmax_inf(v,vhalf,k,tmin){rate_tmax_inf=1.0/(tmin+exp((vhalf-v)/k))}FUNCTIONrate_tmax_fin(v,vhalf,k,tmin,tmax){rate_tmax_fin=1.0/(tmin+1.0/(1.0/(tmax-tmin)+exp((v-vhalf)/k)))}