UNITS{(mA)=(milliamp)(mV)=(millivolt)}NEURON{SUFFIXHHUSEIONnaREADenaWRITEinaUSEIONkREADekWRITEikRANGEgnabar,gkbarGLOBALminf,hinf,ninf,mexp,hexp,nexp}PARAMETER{v(mV)celsius=5(degC)dt(ms)gnabar=0.0(mho/cm2)gkbar=.14(mho/cm2)}STATE{mhn}ASSIGNED{ena(mV)ek(mV)ina(mA/cm2)ik(mA/cm2)minfhinfninfmexphexpnexp}BREAKPOINT{SOLVEstatesina=gnabar*m*m*m*h*(v-ena)ik=gkbar*n*n*n*n*(v-ek)}UNITSOFFINITIAL{rates(v)m=minfh=hinfn=ninf}PROCEDUREstates(){rates(v)m=m+mexp*(minf-m)h=h+hexp*(hinf-h)n=n+nexp*(ninf-n)}PROCEDURErates(v){LOCALq10,tinc,alpha,beta,sumTABLEminf,mexp,hinf,hexp,ninf,nexpDEPENDdt,celsiusFROM-100TO100WITH200q10=3^((celsius-6.3)/10)tinc=-dt*q10alpha=.1*vtrap(-(v+35),10)beta=4*exp(-(v+60)/18)sum=alpha+betaminf=alpha/summexp=1-exp(tinc*sum)alpha=.07*exp(-(v+58)/20)beta=1/(exp(-(v+28)/10)+1)sum=alpha+betahinf=alpha/sumhexp=1-exp(tinc*sum)alpha=.01*vtrap(-(v+34),10)beta=.125*exp(-(v+44)/80)sum=alpha+betaninf=alpha/sumnexp=1-exp(tinc*sum)}FUNCTIONvtrap(x,y){if(fabs(x/y)<1e-6){vtrap=y*(1-x/y/2)}else{vtrap=x/(exp(x/y)-1)}}UNITSON