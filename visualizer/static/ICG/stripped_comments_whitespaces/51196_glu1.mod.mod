NEURON{POINT_PROCESSGrC_Glu1RANGEconccap,gluRANGEtaurec,taufacil,tauin,u0,usr}UNITS{(molar)=(1/liter)(mM)=(millimolar)}PARAMETER{taurec=800(ms)tauin=3(ms)taufacil=20(ms)usr=0.1u0=0<0,1>}ASSIGNED{tx1(ms)conccapconc01(mM)conc02(mM)conc03(mM)vsprglu(mM)glu1(mM)glu2(mM)glu3(mM)}INITIAL{tx1=10000000conc01=0conc02=0conc03=0conccap=0glu=0}BEFOREBREAKPOINT{if(t<tx1){glu=0glu1=0glu2=0glu3=0}if(t>=tx1){glu1=conccap*(2.88(mM)+conc01)*exp((tx1-t)/0.05(ms))glu2=conccap*(0.2(mM)+conc02)*exp((tx1-t)/0.5(ms))glu3=conccap*(0.04(mM)+conc03)*exp((tx1-t)/1.7(ms))glu=glu1+glu2+glu3}}NET_RECEIVE(weight,Eav,R,u,tsyn(ms)){INITIAL{R=1Eav=0u=u0tsyn=t}vspr=((1-R-Eav)/taurec+(R-1)/tauin)/(1/tauin-1/taurec)R=1+exp((tsyn-t)/taurec)*vspr+exp((tsyn-t)/tauin)*(R-1-vspr)Eav=Eav*exp((tsyn-t)/tauin)if(taufacil>0){u=u*exp((tsyn-t)/taufacil)}else{u=usr}if(taufacil>0){u=u+usr*(1-u)}Eav=Eav+R*uconccap=(u*R/usr)*weightR=R-u*Rtsyn=tconc01=glu1conc02=glu2conc03=glu3tx1=t}