NEURON{SUFFIXnapUSEIONnaREADenaWRITEinaRANGEgbarGLOBALtaum_min}UNITS{(S)=(siemens)(mV)=(millivolt)(mA)=(milliamp)}PARAMETER{gbar=33e-6(S/cm2)<0,1e9>taum_min=1.0(ms)}ASSIGNED{ena(mV)ina(mA/cm2)v(mV)g(S/cm2)minftau_m(ms)}STATE{m}BREAKPOINT{SOLVEstatesMETHODcnexpg=gbar*mina=g*(v-ena)}INITIAL{m=alpham(v)/(alpham(v)+betam(v))}DERIVATIVEstates{rates(v)m'=(minf-m)/tau_m}FUNCTIONalpham(Vm(mV))(/ms){UNITSOFFalpham=0.12*exp(0.12*(Vm+56))UNITSON}FUNCTIONbetam(Vm(mV))(/ms){UNITSOFFbetam=0.12*exp(-0.03*(Vm+56))UNITSON}FUNCTIONtaum(Vm(mV))(/ms){UNITSOFFtaum=1.0/(alpham(Vm)+betam(Vm))if(taum<taum_min){taum=taum_min}UNITSON}PROCEDURErates(Vm(mV)){tau_m=taum(Vm)minf=alpham(Vm)/(alpham(Vm)+betam(Vm))}