NEURON{SUFFIXbkfastUSEIONkREADekWRITEikRANGEgbar}UNITS{(S)=(siemens)(mV)=(millivolt)(mA)=(milliamp)}PARAMETER{gbar=37530e-6(S/cm2)<0,1e9>}ASSIGNED{ek(mV)ik(mA/cm2)v(mV)g(S/cm2)ninftau_n(ms)}STATE{n}BREAKPOINT{SOLVEstatesMETHODcnexpg=gbar*n*nik=g*(v-ek)}INITIAL{n=alphan(v)/(alphan(v)+betan(v))}DERIVATIVEstates{rates(v)n'=(ninf-n)/tau_n}FUNCTIONalphan(Vm(mV))(/ms){UNITSOFFalphan=5.82/(1+exp(-0.125*(Vm+3.3)))UNITSON}FUNCTIONbetan(Vm(mV))(/ms){UNITSOFFbetan=2.413/(1+exp(0.0675*(Vm+46.35)))UNITSON}FUNCTIONtaun(Vm(mV))(/ms){UNITSOFFtaun=1.0/(alphan(Vm)+betan(Vm))UNITSON}PROCEDURErates(Vm(mV)){tau_n=taun(Vm)ninf=alphan(Vm)*tau_n}