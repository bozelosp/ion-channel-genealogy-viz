NEURON{SUFFIXNafUSEIONnaREADenaWRITEinaRANGEgNafbar,gna}UNITS{(mA)=(milliamp)(mV)=(millivolt)}PARAMETER{gNafbar=0.086(mho/cm2)<0,1e9>}ASSIGNED{ena(mV)v(mV)ina(mA/cm2)gna(mho/cm2)}STATE{mh}INITIAL{m=malf(v)/(malf(v)+mbet(v))h=half(v)/(half(v)+hbet(v))}BREAKPOINT{SOLVEstatesMETHODderivimplicitgna=gNafbar*m*m*m*hina=gna*(v-ena)}DERIVATIVEstates{m'=(1-m)*malf(v)-m*mbet(v)h'=(1-h)*half(v)-h*hbet(v)}UNITSOFFFUNCTIONmalf(v(mV))(/ms){LOCALvava=v+28if(fabs(va)<1e-04){malf=-0.2816*(-9.3-va*0.5)}else{malf=-0.2816*va/(-1+exp(-va/9.3))}}FUNCTIONmbet(v(mV))(/ms){LOCALvbvb=v+1if(fabs(vb)<1e-04){mbet=0.2464*(6-vb*0.5)}else{mbet=0.2464*vb/(-1+exp(vb/6))}}FUNCTIONhalf(v(mV))(/ms){half=0.098*exp(-(v+43.1)/20)}FUNCTIONhbet(v(mV))(/ms){hbet=1.4/(1+exp(-(v+13.1)/10))}UNITSON