NEURON{SUFFIXnothing}FUNCTIONget_x_pos(gid,gmin,BinNumX,BinNumYZ,binSizeX){LOCALCellNum,tmpCellNum=gid-gmin+1tmp=floor((CellNum-1)/BinNumYZ)get_x_pos=fmod(tmp,BinNumX)*binSizeX+binSizeX/2.0}FUNCTIONget_y_pos(gid,gmin,BinNumY,BinNumZ,binSizeY){LOCALCellNum,tmp,posCellNum=gid-gmin+1tmp=floor((CellNum-1)/BinNumZ)get_y_pos=fmod(tmp,BinNumY)*binSizeY+binSizeY/2.0}FUNCTIONget_z_pos(gid,gmin,BinNumZ,binSizeZ,ZHeight){LOCALCellNum,posCellNum=gid-gmin+1get_z_pos=fmod((CellNum-1),BinNumZ)*binSizeZ+binSizeZ/2+ZHeight}