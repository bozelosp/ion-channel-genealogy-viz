UNITS{(mA)=(milliamp)(mV)=(millivolt)}PARAMETER{v(mV)ek=-90(mV)celsius(degC)gkdrbar=.003(mho/cm2)vhalfn=-32(mV)vhalfl=-61(mV)a0l=0.001(/ms)a0n=0.03(/ms)zetan=-5(1)zetal=2(1)gmn=0.4(1)gml=1.0(1)}NEURON{SUFFIXborgkdrUSEIONkREADekWRITEikRANGEgkdrbar,gkdrGLOBALninf,linf,taun,taul}STATE{nl}ASSIGNED{ik(mA/cm2)ninflinfgkdrtauntaul}INITIAL{rates(v)n=ninfl=linf}BREAKPOINT{SOLVEstatesMETHODcnexpgkdr=gkdrbar*n^3*lik=gkdr*(v-ek)}FUNCTIONalpn(v(mV)){alpn=exp(1.e-3*zetan*(v-vhalfn)*9.648e4/(8.315*(273.16+celsius)))}FUNCTIONbetn(v(mV)){betn=exp(1.e-3*zetan*gmn*(v-vhalfn)*9.648e4/(8.315*(273.16+celsius)))}FUNCTIONalpl(v(mV)){alpl=exp(1.e-3*zetal*(v-vhalfl)*9.648e4/(8.315*(273.16+celsius)))}FUNCTIONbetl(v(mV)){betl=exp(1.e-3*zetal*gml*(v-vhalfl)*9.648e4/(8.315*(273.16+celsius)))}DERIVATIVEstates{rates(v)n'=(ninf-n)/taunl'=(linf-l)/taul}PROCEDURErates(v(mV)){LOCALa,q10q10=3^((celsius-30)/10)a=alpn(v)ninf=1/(1+a)taun=betn(v)/(q10*a0n*(1+a))a=alpl(v)linf=1/(1+a)taul=betl(v)/(q10*a0l*(1+a))}