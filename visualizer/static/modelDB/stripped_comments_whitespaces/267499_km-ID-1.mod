NEURON{SUFFIXkmUSEIONkREADekWRITEikRANGEgkm,ik,alpha,beta}UNITS{(molar)=(1/liter)(mV)=(millivolt)(mA)=(milliamp)(mM)=(millimolar)}INDEPENDENT{tFROM0TO1WITH100(ms)}PARAMETER{gkm=0.006(mho/cm2)}ASSIGNED{ik(mA/cm2)alpha(/ms)beta(/ms)ek(mV)v(mV)}STATE{m}BREAKPOINT{SOLVEstatesMETHODcnexpik=gkm*m*(v-ek)}INITIAL{settables(v)m=alpha/(alpha+beta)ik=gkm*m*(v-ek)}DERIVATIVEstates{settables(v)m'=alpha*(1-m)-beta*m}PROCEDUREsettables(v(mV)){TABLEalpha,betaFROM-120TO40WITH641alpha=m_inf(v)/tau_act(v)beta=(1-m_inf(v))/tau_act(v)}FUNCTIONm_inf(v(mV)){m_inf=1/(1+exp(-(v+35-2)/5))}FUNCTIONtau_act(v(mV)){tau_act=1000/(3.3*exp((v+35)/40)+exp(-(v+35)/20))}